# Environment variables for MCP Scraper

# Optional: Proxy URL for requests (e.g., http://proxy:8080, socks5://proxy:1080)
PROXY_URL=

# Default timeout for requests in seconds
DEFAULT_TIMEOUT=30

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Maximum number of retries for failed requests
MAX_RETRIES=3
